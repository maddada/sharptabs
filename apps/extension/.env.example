# Chrome Extension Manifest (required for extension to work)
# Get your own extension key by loading the unpacked extension once,
# then copy the key from chrome://extensions
VITE_EXTENSION_KEY=your_extension_key

# OAuth2 Client ID for Google Sign-In
# Create at https://console.cloud.google.com/apis/credentials
VITE_OAUTH_CLIENT_ID=your_oauth_client_id.apps.googleusercontent.com

VITE_PEXELS_API_KEY=your_pexels_api_key

VITE_APP_FIREBASE_API_KEY=your_firebase_api_key
VITE_APP_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_APP_FIREBASE_PROJECT_ID=your-project-id
VITE_APP_FIREBASE_STORAGE_BUCKET=your-project.firebasestorage.app
VITE_APP_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
VITE_APP_FIREBASE_APP_ID=your_app_id
