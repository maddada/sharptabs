{
    "name": "@apps/extension",
    "private": true,
    "version": "0.1.0",
    "license": "GPL-3.0",
    "type": "module",
    "scripts": {
        "dev": "tsc && vite build --mode development",
        "build": "tsc && vite build --mode production",
        "obfuscate": "javascript-obfuscator dist/assets --output dist/assets --config obfuscator.config.json",
        "build-OLD": "npm run build:basic && npm run obfuscate && npm run compile-content-script",
        "dev:vite": "vite",
        "devtools": "react-devtools",
        "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "typecheck": "tsc --noEmit",
        "preview": "vite preview",
        "format": "prettier --write \"src/**/*.{ts,tsx,css,md,json}\"",
        "format:all": "prettier --write .",
        "test": "vitest",
        "convex": "convex dev",
        "healthcheck": "npx react-compiler-healthcheck",
        "postinstall": "patch-package",
        "compile-content-script": "tsc src/content_script.ts --outDir dist/assets --target ES2020 --module ESNext --skipLibCheck"
    },
